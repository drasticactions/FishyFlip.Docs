{
  "metadata": [
    {
      "src": [
        {

          "src": "..",
          "files": [
            "external/FishyFlip/src/FishyFlip/*.csproj",
            "external/FishyFlip/src/FishyFlip.Xrpc/*.csproj",
            "external/FishyFlip/src/FishyFlip.AspNetCore/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "outputFormat": "apiPage"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "main.css",
          "ff-favicon.ico"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "fishyflip"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appName": "FishyFlip",
      "_appTitle": "FishyFlip",
      "_enableSearch": true,
      "pdf": true,
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "ff-favicon.ico"
    }
  }
}